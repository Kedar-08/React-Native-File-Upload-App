      }
    },
    "render": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "https://file-upload-app-4wxc.onrender.com"
      }
    },
    "production": {
